ex',alignItems:'center'},children:farm.apy?/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(jsx_runtime[\"Fragment\"],{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(FarmCard_ApyButton,{lpLabel:lpLabel,quoteTokenAdresses:quoteTokenAdresses,quoteTokenSymbol:quoteTokenSymbol,tokenAddresses:tokenAddresses,cakePrice:cakePrice,apy:farm.apy}),farmAPY,\"%*\"]}):/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(index_esm[\"y\" /* Skeleton */],{height:24,width:80})})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(index_esm[\"m\" /* Flex */],{justifyContent:\"space-between\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(index_esm[\"B\" /* Text */],{style:{fontSize:'12px'},children:[\" \",TranslateString(318,'Earn'),\":\"]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(index_esm[\"B\" /* Text */],{bold:true,children:earnLabel})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(index_esm[\"m\" /* Flex */],{justifyContent:\"space-between\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(index_esm[\"B\" /* Text */],{style:{fontSize:'9px'},children:[TranslateString(10001,'Deposit Fee'),\":\"]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(index_esm[\"B\" /* Text */],{bold:true,style:{fontSize:'15px'},children:[farm.depositFeeBP/100,\"%\"]})]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(CardActionsContainer,{farm:farm,ethereum:ethereum,account:account}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Divider,{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(components_ExpandableSectionButton_ExpandableSectionButton,{onClick:function onClick(){return setShowExpandableSection(!showExpandableSection);},expanded:showExpandableSection}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(ExpandingWrapper,{expanded:showExpandableSection,children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(FarmCard_DetailsSection,{removed:removed,isTokenOnly:farm.isTokenOnly,bscScanAddress:farm.isTokenOnly?\"https://bscscan.com/token/\".concat(farm.tokenAddresses[\"56\"]):\"https://bscscan.com/token/\".concat(farm.lpAddresses[\"56\"])